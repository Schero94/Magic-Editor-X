    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19"
  },
  "overrides": {
    "prismjs": "^1.30.0"
  },
  "strapi": {
    "kind": "plugin",
    "name": "magic-editor-x",
    "displayName": "Magic Editor X",
    "description": "Advanced block-based editor with Editor.js for Strapi v5"
  },
